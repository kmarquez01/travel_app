{
  "expo": {
    "name": "Oterra",
    "scheme": "oterra",
    "slug": "Oterra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.oterra",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location for mapping features.",
        "MGLMapboxAccessToken": "YOUR_MAPBOX_ACCESS_TOKEN"
      }
    },
    "android": {
      "package": "com.anonymous.oterra",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAroT3uGDrF7dOmFB-71LQiAfpx9MyyIYU"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "gradleProperties": {
        "MGLMapboxAccessToken": "YOUR_MAPBOX_ACCESS_TOKEN"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      "@react-native-mapbox-gl/maps"
    ]
  }
}
